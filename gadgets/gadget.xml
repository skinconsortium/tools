<gadget>
  <name>Mensaplan München</name>
  <namespace>microsoft.windows</namespace>
  <version>1.0</version>
  <author name="Martin Pöhlmann">
    <info url="http://www.skinconsortium.com" text="Skin Consortium"/>
    <logo src="images/logo.png" />
  </author>
  <copyright>&#169; Skin Consortium.</copyright>
  <description>Ein einfaches Gadget, welches den Mensa Speiseplan anzeigt.</description>
  <icons>
    <icon height="48" width="48" src="images/aerologo.png" />
  </icons>
  <hosts>
    <host name="sidebar">
      <base type="HTML" apiVersion="1.0.0" src="Mensa.html" />
      <permissions>Full</permissions>
      <platform minPlatformVersion="1.0" />
      <defaultImage src="images/aerologo.png" />
    </host>
  </hosts>
</gadget>